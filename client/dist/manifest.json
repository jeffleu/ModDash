{
  "manifest_version": 2,
  "name": "ModDash",
  "description": "Never be late again",
  "version": "1.0.0",
  "background": {
    "persistent": true,
    "page": "./chrome/background.html"
  },
  "chrome_url_overrides": {
    "newtab": "./index.html"
  },
  "permissions": [
    "notifications",
    "background",
    "identity",
    "storage",
    "geolocation"
  ],
  "oauth2": {
    "client_id": "620726441303-ba0bp99k03erdsr829sclks4cttst15u.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/plus.login", "https://www.googleapis.com/auth/plus.profile.emails.read"]
  },
  "key": "",
  "content_security_policy": "script-src 'self' https://www.googleapis.com https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.14.1/ https://cdn.pubnub.com/ http://velocitydash.com http://www.velocitydash.com  https://maxcdn.bootstrapcdn.com/bootstrap/latest/css/bootstrap.min.css; object-src 'self' "
}
